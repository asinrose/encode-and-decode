 #include <stdio.h>
int main()
{
    int age,r,code;  //declare variables
    printf("enter the age: ");
    scanf("%d",&age);        //input the content
    printf("enter the roll no: ");
    scanf("%d",&r);
    code=age^r;       //encode using XOR
    printf("the roll no is %d",code^age);  
    printf("the age is %d",code^r);
    return 0;
}
